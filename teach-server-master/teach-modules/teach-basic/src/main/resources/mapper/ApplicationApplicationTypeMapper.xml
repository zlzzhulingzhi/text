<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.qbs.wa.teach.basic.mapper.ApplicationApplicationTypeMapper">

    <resultMap type="cn.qbs.wa.teach.basic.entity.ApplicationApplicationType" id="ApplicationApplicationTypeMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="appId" column="app_id" jdbcType="INTEGER"/>
        <result property="appTypeId" column="app_type_id" jdbcType="INTEGER"/>
        <result property="createBy" column="create_by" jdbcType="INTEGER"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <!-- 批量插入 -->
    <insert id="insertBatch" keyProperty="id" useGeneratedKeys="true">
        insert into application_application_type(app_id, app_type_id, create_by, create_time)
        values
        <foreach collection="entities" item="entity" separator=",">
        (#{entity.appId}, #{entity.appTypeId}, #{entity.createBy}, #{entity.createTime})
        </foreach>
    </insert>

    <!-- 批量插入或按主键更新 -->
    <insert id="insertOrUpdateBatch" keyProperty="id" useGeneratedKeys="true">
        insert into application_application_type(app_id, app_type_id, create_by, create_time)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.appId}, #{entity.appTypeId}, #{entity.createBy}, #{entity.createTime})
        </foreach>
        on duplicate key update
        app_id = values(app_id) , app_type_id = values(app_type_id) , create_by = values(create_by) , create_time = values(create_time)         $empty
    </insert>

    <select id="page" resultType="cn.qbs.wa.teach.basic.pojo.applicationapplicationtype.ApplicationApplicationTypePageResponse">
        select *
        from application_application_type
        <where>
            <!-- 示例代码，自行修改 -->
            <!--
            <if test="params.name != null and params.name != '' ">
                and name like concat('%', #{params.name}, '%')
            </if>
            -->
        </where>
    </select>

    <select id="selectDetailById" resultType="cn.qbs.wa.teach.basic.pojo.applicationapplicationtype.ApplicationApplicationTypeDetailResponse">
        select *
        from application_application_type
        where id = #{id}
    </select>
</mapper>

